{
  "name": "Slurp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --updateSnapshot && jest",
    "testCoverage": "jest --updateSnapshot && jest --coverage",
    "coverage": "jest --updateSnapshot && jest --coverage",
    "stripe": "cd server && npm start",
    "lint": "eslint .",
    "pod": "cd ios && pod install && cd ..",
    "refresh": "git pull && npm ci && cd android && ./gradlew clean && cd .. && react-native link && cd ios && pod install && cd ..",
    "softRefresh": "npm ci && cd android && ./gradlew clean && cd .. && react-native link && cd ios && pod install && cd .."
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/checkbox": "^0.5.9",
    "@react-native-firebase/app": "^14.5.1",
    "@react-native-firebase/auth": "^14.5.0",
    "@react-native-firebase/firestore": "^14.5.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/material-top-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@stripe/stripe-react-native": "^0.3.0",
    "react": "17.0.2",
    "react-input-mask": "^2.0.4",
    "react-native": "0.67.2",
    "react-native-animatable": "^1.3.3",
    "react-native-collapsible-tab-view": "^4.5.2",
    "react-native-device-info": "^8.4.9",
    "react-native-elements": "^3.4.2",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "2.2.0",
    "react-native-get-location": "^2.2.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.30.1",
    "react-native-pager-view": "^5.4.11",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.13.1",
    "react-native-scroll-bottom-sheet": "*",
    "react-native-shadow-2": "^6.0.2",
    "react-native-shared-element": "^0.8.4",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.1",
    "react-native-tabs-section-list": "^1.0.1",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation": "^4.4.4",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@babel/runtime": "7.16.7",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "26.6.3",
    "eslint": "7.14.0",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.66.2",
    "msw": "^0.24.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "react-native"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__testingAid__/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__testingAid__/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native)"
    ],
    "setupFiles": [
      "<rootDir>/__testingAid__/jest.setup.js"
    ]
  }
}
