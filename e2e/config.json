{
    "maxWorkers": 1,
    "testEnvironment": "./environment",
    "testRunner": "jest-circus/runner",
    "testTimeout": 120000,
    "testRegex": "\\.e2e\\.js$",
    "reporters": ["detox/runners/jest/streamlineReporter"],
    "verbose": true,
    "globals": {
        "__DEV__": true
    },
    "transform": {
        "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "moduleDirectories": ["node_modules", "src"],
    "transformIgnorePatterns": [
        "/node_modules/(?!react-native|react-navigation|rn-fetch-blob|@myob|deprecated-react-native-listview|@react-native-firebase/firestore|@react-native-firebase/auth|@react-native-firebase/app)"
    ]
}
